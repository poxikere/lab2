open System
let rec vvod acc =
    printf "Введите цифру (или 'net' для выхода): "
    let input = Console.ReadLine()
    match input with
    | "net" -> List.rev acc
    | s ->
        match Int32.TryParse(s) with
        | true, num -> (if num<10 && num>0 then vvod (input :: acc)
            else printfn "Некорректный ввод. Пожалуйста, введите цифру или 'net'."
                 vvod acc)
        | false, _ ->
            printfn "Некорректный ввод. Пожалуйста, введите цифру или 'net'."
            vvod acc

let rim x =
    match x with
    | "1" -> "I"
    | "2" -> "II"
    | "3" -> "III"
    | "4" -> "IV"
    | "5" -> "V"
    | "6" -> "VI"
    | "7" -> "VII"
    | "8" -> "VIII"
    | "9" -> "IX"

let smena xx =
    xx |> List.map (rim)

let list1 = vvod []
let list2 = smena list1
    
printfn "Арабские цифры: %A" list1
printfn "Римские цифры: %A" list2
